{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "dictionaries": [
    "software-terms",
    "npm",
    "fullstack",
    "redis",
    "typescript",
    "softwareTerms",
    "node",
    "en_us",
    "en-gb",
    "npm",
    "html",
    "companies",
    "misc",
    "filetypes",
    "bash",
    "lorem-ipsum",
    "es-es"
  ],
  "language": "en",
  "files": ["**", ".vscode/**", ".github/**"],
  "ignorePaths": ["pnpm-lock.yaml", ".pnpm-store/", "*.sql", ".vscode/**", ".coderabbit.yaml", ".cursor/**"],
  "allowCompoundWords": true,
  "import": [
    "@cspell/dict-redis/cspell-ext.json",
    "@cspell/dict-typescript",
    "@cspell/dict-software-terms",
    "@cspell/dict-node",
    "@cspell/dict-en_us",
    "@cspell/dict-en-gb",
    "@cspell/dict-npm",
    "@cspell/dict-html",
    "@cspell/dict-companies",
    "@cspell/dict-filetypes",
    "@cspell/dict-bash",
    "@cspell/dict-lorem-ipsum/cspell-ext.json",
    "@cspell/dict-es-es/cspell-ext.json"
  ],
  "useGitignore": true,
  "version": "0.2",
  "words": ["rslib", "sonarjs", "willgravinadev"]
}
