{
  "name": "@github-search/eslint-config",
  "version": "0.1.38",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "check:lint": "eslint . --max-warnings 0",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf .turbo dist node_modules build",
    "dev": "rslib build --watch",
    "fix:lint": "eslint --fix ."
  },
  "lint-staged": {
    "*.{cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": "eslint --fix"
  },
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^18.3.0",
    "@angular-eslint/eslint-plugin-template": "^18.3.0",
    "@angular-eslint/template-parser": "^18.3.0",
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@eslint/js": "^9.32.0",
    "@next/eslint-plugin-next": "^15.4.4",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^3.7.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "eslint-plugin-testing-library": "^7.6.1",
    "eslint-plugin-turbo": "^2.5.5",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "local-pkg": "^1.1.1"
  },
  "devDependencies": {
    "@github-search/typescript-config": "workspace:*",
    "eslint": "^9.32.0"
  },
  "peerDependencies": {
    "eslint": "^9.20.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
